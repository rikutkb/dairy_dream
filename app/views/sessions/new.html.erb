<% provide(:title, 'ログイン') %>

<h1>Sessions#new</h1>
<div class="row">
    <%= form_for(:session,url:login_path)do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name,class:'form-control' %>

    <%= f.label :password %>
    <%= f.password_field :password,class:'form-control' %>
    <%= f.submit "Login" %>
    <% end %>

</div>

